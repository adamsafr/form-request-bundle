{
    "name": "adamsafr/form-request-bundle",
    "description": "An adaptation of the Laravel Form Request for Symfony",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Sapraliev",
            "email": "sapraliev@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1.3",
        "symfony/config": "^3.4 || ^4.0 || ^5.0",
        "symfony/dependency-injection": "^3.4 || ^4.0 || ^5.0",
        "symfony/http-kernel": "^3.4 || ^4.0 || ^5.0",
        "symfony/validator": "^3.4 || ^4.0 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "Adamsafr\\FormRequestBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adamsafr\\FormRequestBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "dg/bypass-finals": "^1.1",
        "symfony/var-dumper": "^3.4 || ^4.0 || ^5.0",
        "symfony/phpunit-bridge": "^3.4 || ^4.0 || ^5.0",
        "symfony/translation": "^3.4 || ^4.0 || ^5.0"
    }
}
